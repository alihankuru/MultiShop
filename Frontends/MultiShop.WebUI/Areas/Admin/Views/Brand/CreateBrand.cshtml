@model CreateBrandDto
@{
    ViewData["Title"] = "CreateBrand";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}


<section class="content container-fluid">
    <div class="row">

        <div class="col-md-12">
            <div class="chart-box">
                <h4>Bordered Table</h4>
                <p>Add <code>.table-bordered</code> for borders on all sides of the table and cells.</p>
                <div class="bs-example" data-example-id="bordered-table">
                    <form method="post">
                        <label>Başlık Adı</label>
                        <input type="text" class="form-control" asp-for="BrandName" />
                        <br />
                        <label>Icon Adı</label>
                        <input type="text" class="form-control" asp-for="ImageUrl" />
                        <br />
                        <button class="btn btn-primary">Marka Ekle</button>
                    </form>
                </div>
            </div>
        </div>

    </div>
</section>
